{

    "description": "Request observer for firefox",
    "manifest_version": 2,
    "name": "Request Magnifier",
    "version": "1.0",
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "https://developer.mozilla.org/",
      "https://mdn.mozillademos.org/",
      "<all_urls>"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Request Magnifier",
        "default_popup": "popup.html"
    },

    "background": {
      "scripts": ["background.js"]
    }
  
  }